pan>              {/if}              <input class="sel_move" style="display:none" name="mov" type="submit" name="" value="{$LANG.CONTINUE}" />              <input class="sel_move" style="display:none" type="button" name="" value="{$LANG.CANCEL}" onclick="$('.sel_move').toggle();$('.sel_pub').toggle();" />          </div>          </form>      {/if}      {$pagebar}{else}    	<p class="not_found">        {if $channel.view=='latest'}        	{$LANG.NOT_AVAILABLE_MOVIE}        {elseif $channel.view=='fav'}        	{$LANG.NOT_FAV_MOVIE}        {elseif $channel.view=='on_convert'}        	{$LANG.NOT_MOVIE_ON_CONVERT}		{/if}        </p>      {if $channel.view=='latest' && $access.is_auth && ($access.myprofile || $access.is_admin || $access.is_moder) && $album.id}          <div class="usr_photo_sel_bar bar">            <input type="button" value="{$LANG.EDIT_ALBUM}" onclick="core.message('{$LANG.EDIT_ALBUM}');$('#popup_message').html($('#edit_album').html());$('#popup_progress, #popup_panel').hide();" />            <input type="button" value="{$LANG.DELETE_ALBUM}" onclick="core.confirm('{$LANG.DEL_ALBUM_CONFIRM}', null, function() {literal}{{/literal} window.location.href = '/video/delete_album{$album.id}.html'; {literal}}{/literal})" />          </div>      {/if}{/if}		</div>	<div class="col-md-4">		<div class="panel panel-default">			<div class="panel-heading">				<h4 class="panel-title">{$LANG.CHANNELS_AUTHOR}</h4>			</div>			<div class="panel-body">				<div class="media">					<a class="pull-left" href="{profile_url login=$usr.login}" title="{$usr.nickname|escape:'html'}"><img class="media-object" src="{$usr.imageurl}" alt="{$usr.nickname|escape:'html'}" /></a>					<div class="media-body">						<h4 class="media-heading"><a href="{profile_url login=$usr.login}" title="{$usr.nickname|escape:'html'}">{$usr.nickname}</a></h4>                        {if !$access.myprofile && $access.is_auth}						<div class="media-hinttext">							{add_js file='components/users/js/profile.js'}							<span class="monospc"><a class="ajaxlink" href="javascript:void(0)" title="{$LANG.WRITE_MESSAGE}: {$usr.nickname|escape:'html'}" onclick="users.sendMess('{$usr.id}', 0, this);return false;">{$LANG.WRITE_MESSAGE}</a></span>							{if $access.is_friend}							<span class="monospc del_friend_ajax"><a href="javascript:void(0)" title="{$usr.nickname|escape:'html'}" onclick="users.delFriend('{$usr.id}', this);return false;">{$LANG.MOV_STOP_FRIENDLY}</a></span>							<span class="monospc add_friend_ajax" style="display: none;"><a href="javascript:void(0)" title="{$usr.nickname|escape:'html'}" onclick="users.addFriend('{$usr.id}', this);return false;">{$LANG.MOV_ADD_TO_FRIEND}</a></span>							{else}							<span class="monospc"><a href="javascript:void(0)" title="{$usr.nickname|escape:'html'}" onclick="users.addFriend('{$usr.id}', this);return false;">{$LANG.MOV_ADD_TO_FRIEND}</a></span>							{/if}						</div>                        {/if}					</div>				</div>			</div>		</div>				<div class="panel panel-default">			<div class="panel-heading">				<h4 class="panel-title">{$LANG.OPTIONS_VIEW}</h4>			</div>			<div class="panel-body">					<select name="perpage" id="perpage" onchange="perpage('{$usr.login}', '{$channel.view}');" >                    	<option value="6" {if $channel.perpage=='6'} selected {/if}>{$LANG.MOVIE_PERPAGE}: 6</option>                    	<option value="12" {if $channel.perpage=='12'} selected {/if}>{$LANG.MOVIE_PERPAGE}: 12</option>                        <option value="15" {if $channel.perpage=='15'} selected {/if}>{$LANG.MOVIE_PERPAGE}: 15</option>                        <option value="18" {if $channel.perpage=='18'} selected {/if}>{$LANG.MOVIE_PERPAGE}: 18</option>                        <option value="21" {if $channel.perpage=='21'} selected {/if}>{$LANG.MOVIE_PERPAGE}: 21</option>					</select><br />                    <select name="channel_view" id="channel_view" onchange="viewChannelType('{$usr.login}');">						<option value="list" {if $channel.channel_view=='list'} selected {/if}>{$LANG.VIEW_CHANNEL}: {$LANG.CHANNEL_LIST}</option>						<option value="review" {if $channel.channel_view=='review'} selected {/if}>{$LANG.VIEW_CHANNEL}: {$LANG.CHANNEL_REVIEW}</option>					</select><br />                    <div id="p_query_channel">                        <input name="query_channel" id="query_channel" type="text" value="{$channel.query_channel}" class="text-input" placeholder="{$LANG.SEARCH_BY_CHANNEL}" />                        {if $channel.query_channel && $channel.query_channel != $LANG.SEARCH_BY_CHANNEL}<br />                            <a class="btn btn-default" href="javascript:;" onclick="clearChannelSearch('{$usr.login}');">{$LANG.CANSEL_SEARCH}</a>                        {/if}                    </div>			</div>		</div>			<div class="panel panel-default">			<div class="panel-heading">				<h4 class="panel-title">{$LANG.VIDEOALBUMS}            {if $access.myprofile && $access.is_auth}                <a href="javascript:;" onclick="addAlbum();" class="pull-right badge" title="{$LANG.ADD_ALBUM}">+</a>            {/if}								</h4>			</div>			{if $channel.albums}            <div class="list-group">                <a href="javascript:;" class="list-group-item{if !$album.id} active{/if}" onclick="viewChannel('latest', '{$usr.login}');">{$LANG.ALL_MOVIES}</a>                {foreach key=tid item=users_album from=$channel.albums}                <a class="list-group-item{if $album.id == $users_album.id} active{/if}" href="javascript:;" onclick="viewChannelAlbum({$users_album.id}, '{$usr.login}'); return false;">{$users_album.title} <span class="badge">{$users_album.movie_count}</span></a>                {/foreach}            </div>            {else}			<div class="panel-body">                <p class="text-danger">{$LANG.NOT_VIDEOALBUMS}...</p>			</div>            {/if}		</div>			{if $channel.cats}		<div clas